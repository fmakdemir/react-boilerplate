{
  "name": "fmas-react-boilerplate",
  "version": "1.0.1",
  "description": "My boilerplate using nodejs and and reactjs",
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run build && PORT=3002 forever start server.prod.js",
    "stop": "forever stop server.prod.js",
    "lint": "eslint src",
    "build": "yarn install && NODE_ENV=production webpack",
    "test": "npm run lint",
    "dev": "webpack-dashboard -- node server.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fmakdemir/react-boilerplate.git"
